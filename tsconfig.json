{
    "compilerOptions": {
      "target": "es2022",
      "module": "commonjs",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "outDir": "gen/srv",
      "skipLibCheck": true,
      "sourceMap": true,
      "allowJs": true,
      "strict": true,
      "noImplicitAny": false,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "typeRoots": [
        "node_modules/@types"
      ]
    },
    "include": [
      "srv/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  